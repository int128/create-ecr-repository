{
  "name": "create-ecr-repository-action",
  "version": "0.0.0",
  "private": true,
  "description": "Create Amazon ECR repository if not exist",
  "main": "lib/src/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "check": "yarn format && yarn lint && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/int128/create-ecr-repository-action.git"
  },
  "keywords": [
    "actions",
    "aws"
  ],
  "author": "int128",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.6.0",
    "aws-sdk": "2.1033.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "2.7.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.9",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "@vercel/ncc": "0.32.0",
    "eslint": "8.2.0",
    "eslint-plugin-jest": "25.2.4",
    "jest": "27.3.1",
    "jest-circus": "27.3.1",
    "js-yaml": "4.1.0",
    "prettier": "2.4.1",
    "ts-jest": "27.0.7",
    "typescript": "4.5.2"
  }
}

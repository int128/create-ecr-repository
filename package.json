{
  "name": "create-ecr-repository-action",
  "version": "0.0.0",
  "private": true,
  "description": "Create Amazon ECR repository if not exist",
  "main": "lib/src/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "check": "yarn format && yarn lint && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/int128/create-ecr-repository-action.git"
  },
  "keywords": [
    "actions",
    "aws"
  ],
  "author": "int128",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@aws-sdk/client-ecr": "3.267.0",
    "@aws-sdk/client-ecr-public": "3.267.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.3",
    "@types/jest": "29.4.0",
    "@types/node": "16.18.12",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "@vercel/ncc": "0.36.1",
    "aws-sdk-client-mock": "2.0.1",
    "eslint": "8.34.0",
    "eslint-plugin-jest": "26.9.0",
    "jest": "29.4.2",
    "js-yaml": "4.1.0",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  }
}

  "keywords": [
    "actions",
    "aws"
  ],
  "author": "int128",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.3.0",
    "aws-sdk": "2.922.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "2.7.0",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.2",
    "@typescript-eslint/parser": "4.26.0",
    "@vercel/ncc": "0.28.6",
    "eslint": "7.28.0",
    "eslint-plugin-github": "4.1.3",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "27.0.4",
    "jest-circus": "27.0.4",
    "js-yaml": "4.1.0",
    "prettier": "2.3.1",
    "ts-jest": "27.0.2",
    "typescript": "4.3.2"
  }

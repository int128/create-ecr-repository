{
  "name": "create-ecr-repository-action",
  "version": "0.0.0",
  "private": true,
  "description": "Create Amazon ECR repository if not exist",
  "main": "lib/src/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "check": "yarn format && yarn lint && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/int128/create-ecr-repository-action.git"
  },
  "keywords": [
    "actions",
    "aws"
  ],
  "author": "int128",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.8.2",
    "@aws-sdk/client-ecr": "3.100.0",
    "@aws-sdk/client-ecr-public": "3.100.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.2",
    "@types/jest": "27.5.2",
    "@types/node": "16.11.38",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "@vercel/ncc": "0.34.0",
    "aws-sdk-client-mock": "0.6.2",
    "eslint": "8.17.0",
    "eslint-plugin-jest": "26.5.3",
    "jest": "28.1.0",
    "js-yaml": "4.1.0",
    "prettier": "2.6.2",
    "ts-jest": "28.0.4",
    "typescript": "4.7.3"
  }
}

{
  "name": "create-ecr-repository-action",
  "version": "0.0.0",
  "private": true,
  "description": "Create Amazon ECR repository if not exist",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/int128/create-ecr-repository-action.git"
  },
  "keywords": [
    "actions",
    "aws"
  ],
  "author": "int128",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "aws-sdk": "^2.897.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.28.3",
    "eslint": "^7.17.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "prettier": "2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
